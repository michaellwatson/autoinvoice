RewriteEngine on
RewriteCond $1 !^(index\.php|resources|robots\.txt)
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php/$1 [L,QSA] 

# --
# Redirect PDF requests to the Security controller
RewriteRule ^assets/quotes/(.*)$ index.php/security?f=$1 [L]

# second way
# RewriteRule ^assets/quotes/([A-Za-z0-9_]+\.pdf)$ index.php/security/?f=$1 [L]
